Task 1.
def div(*args):

    try:
        arg1 = int(input("Введите делимое "))
        arg2 = int(input("Введите делитель "))
        res = arg1 / arg2
    except ValueError:
        return 'Ошибка значения'
    except ZeroDivisionError:
        return "Нельзя делить на ноль, выбирите другой делитетль "

    return res
print(f'Результат:  {div()}')

